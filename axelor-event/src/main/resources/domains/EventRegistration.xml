<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

		<module name="event" package="com.axelor.apps.event.db" />

		<entity name="EventRegistrations" lang="java" >

				<many-to-one name="event" ref="com.axelor.apps.event.db.Events" />
				<string name="name" required="true" title="Name" />
				<string name="email" required="true" title="Email" />

				<many-to-one name="address" ref="com.axelor.apps.event.db.EventAddress"
						title="Address" />
				<datetime name="registrationDate" required="true" title="Registration Date" />

				<decimal name="amount" readonly="true" title=" Amount" precision="20" scale="2" />

				<boolean name="isSendEmail" default="false" />

		</entity>

</domain-models>
