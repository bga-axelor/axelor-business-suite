<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

		<grid model="com.axelor.gst.db.State" title="State" name="state-grid">
				<toolbar>
						<button name="btnImport" title="Import" onClick="action-method-gst-import" />

				</toolbar>

				<field name="name" />
				<field name="country" />
		</grid>

		<form name="state-form" title="State" model="com.axelor.gst.db.State" css="panel-blue">
				<panel>
						<field name="name" />
						<field name="country" selection="state.country.selection" />
				</panel>
		</form>

		<action-method name="action-method-gst-import" model="com.axelor.gst.db.State">
				<call class="com.axelor.apps.gst.web.ImportController" method="importCSVData" />
		</action-method>



</object-views>