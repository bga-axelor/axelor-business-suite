<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

		<module name="sale" package="com.axelor.apps.sale.db" />

		<entity name="SaleOrderLine" lang="java">

				<string name="hsbn" title="HSBN" />
				<decimal name="gstRate" required="true" readonly="true" title="GST Rate" precision="20"
						scale="2" />
				<decimal name="igst" readonly="true" title="IGST" precision="20" scale="2" />
				<decimal name="sgst" readonly="true" title="SGST" precision="20" scale="2" />
				<decimal name="cgst" readonly="true" title="CGST" precision="20" scale="2" />
				<decimal name="grossAmount" readonly="true" title="Gross Amount" precision="20"
						scale="2" />

		</entity>

</domain-models>
